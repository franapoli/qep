## The function is currently defined as
function (qmix, i, v = NULL) 
{
    if (is.null(v)) 
        v <- 1:ncol(qmix[[i]])
    l <- cumsum(c(0, sapply(qmix, ncol)))
    return(v + l[i])
  }
}
